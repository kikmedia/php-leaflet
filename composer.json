{
    "name": "netzmacht/php-leaflet",
    "description": "PHP leaflet definition and javascript generator",
    "keywords": ["leaflet", "maps", "javascript", "library"],
    "type": "library",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "David Molineus",
            "email": "david.molineus@netzmacht.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "symfony/event-dispatcher": "~2.3",
        "netzmacht/php-javascript-builder": "~1.0",
        "beberlei/assert": "~2.0"
    },
    "require-dev": {
        "netzmacht/contao-build-tools": "~1.1"
    },
    "autoload": {
        "psr-0": {
            "Netzmacht\\LeafletPHP\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-support/0.7.x": "0.7.x-dev",
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.1.x-dev"
        }
    }
}
